    "@remix-run/react": "^1.7.5",
    "@remix-run/vercel": "^1.7.5",
    "@vercel/node": "^2.6.2",
    "moviedb-promise": "^3.3.1",
    "phosphor-react": "^1.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.7.5",
    "@remix-run/eslint-config": "^1.7.5",
    "@remix-run/serve": "^1.7.5",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "eslint": "^8.27.0",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.8.4"
